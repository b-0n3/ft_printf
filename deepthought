TESTED : "%u", 100
printf :    |100	Return: 3
ft_printf : |	Return: -1

TESTED : "%u", UINT32_MAX
printf :    |4294967295	Return: 10
ft_printf : |	Return: -1

TESTED : "%u", UINT32_MAX + 95
printf :    |94	Return: 2
ft_printf : |	Return: -1

TESTED : "%u", 0
printf :    |0	Return: 1
ft_printf : |	Return: -1

TESTED : "a%ua", 100
printf :    |a100a	Return: 5
ft_printf : |aa	Return: 0

TESTED : "a%ua", UINT32_MAX
printf :    |a4294967295a	Return: 12
ft_printf : |aa	Return: 0

TESTED : "a%ua", 0
printf :    |a0a	Return: 3
ft_printf : |aa	Return: 0

