printf :    |Hello	Return: 5
ft_printf : |	Return: -1

TESTED : "%s", ""
printf :    |	Return: 0
ft_printf : |	Return: -1

TESTED : "%s", NULL
printf :    |(null)	Return: 6
ft_printf : |	Return: -1

TESTED : "a%sa", "Hello"
printf :    |aHelloa	Return: 7
ft_printf : |aa	Return: 0

TESTED : "a%sa", ""
printf :    |aa	Return: 2
ft_printf : |aa	Return: 0

TESTED : "a%sa", NULL
printf :    |a(null)a	Return: 8
ft_printf : |aa	Return: 0

TESTED : "%s", "%15d"
printf :    |%15d	Return: 4
ft_printf : |	Return: -1

